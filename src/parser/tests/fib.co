// Fibonacci numbers
// using a while loop
var a = 0;
var b = 1;
var j = 0;
while (j < 6) {
  print(a);
  var temp = a;
  a = b;
  b = temp + b;
  j = j + 1;
}

// Fibonacci numbers
// using recursive function call
function fib(n) {
  if (n < 2) {
    return n;
  }
  return fib(n - 2)
    + fib(n - 1);
}

var i = 0;
while (i < 6) {
  print(fib(i));
  i = i + 1;
}